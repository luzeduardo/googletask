    "webpack-livereload-plugin": "^1.0.0"
  },
  "scripts": {
    "dev": "webpack --mode development --module-bind js=babel-loader",
    "build": "webpack --mode production --optimize-minimize --module-bind js=babel-loader",
    "watch": "webpack --mode development --module-bind js=babel-loader --watch",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "coverage": "node node_modules/jest/bin/jest.js --watch --silent --coverage"
  },
  "jest": {
    "resolver": "jest-webpack-resolver",
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    },
    "verbose": true,
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "collectCoverage": false,
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/dist/**"
    ],
    "modulePathIgnorePatterns": [
      "coverage"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/jestSetup.js"
  }
}
